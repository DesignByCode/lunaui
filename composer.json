{
    "name": "designbycode/lunaui",
    "description": "Luna-sass preset for Laravel 6",
    "keywords": ["laravel", "luna-sass", "ui"],
    "license": "MIT",
    "authors": [
        {
            "name": "Claude Myburgh",
            "email": "claude@deisgnbycode.co.za"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "illuminate/console": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "DesignByCode\\LunaUi\\": "src/",
            "Illuminate\\Foundation\\Auth\\": "auth-backend/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesignByCode\\LunaUi\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "DesignByCode\\LunaUi\\LunaUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
